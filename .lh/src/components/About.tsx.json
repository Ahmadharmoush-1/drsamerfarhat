            "content": "import { Award } from \"lucide-react\";\r\nimport { Button } from \"@/components/ui/button\";\r\nimport doctorAbout from \"@/assets/doctor-about.jpg\";\r\n\r\nconst About = () => {\r\n  const scrollToAppointment = () => {\r\n    const element = document.querySelector(\"#appointment\");\r\n    if (element) {\r\n      element.scrollIntoView({ behavior: \"smooth\" });\r\n    }\r\n  };\r\n\r\n  return (\r\n    <section id=\"about\" className=\"py-20 bg-card\">\r\n      <div className=\"container mx-auto px-4\">\r\n        <div className=\"grid lg:grid-cols-2 gap-12 items-center\">\r\n          {/* Left - Doctor Image */}\r\n          <div className=\"relative fade-in-up\">\r\n            <div className=\"relative z-10\">\r\n              <img\r\n                src={doctorAbout}\r\n                alt=\"Dr. Samer Farhat\"\r\n                className=\"w-full h-auto rounded-3xl shadow-2xl\"\r\n              />\r\n            </div>\r\n\r\n            {/* Experience Badge */}\r\n            <div className=\"absolute -bottom-6 -right-6 bg-primary text-primary-foreground rounded-2xl shadow-2xl p-6 animate-scale-in animate-delay-200\">\r\n              <div className=\"flex items-center gap-3\">\r\n                <Award className=\"w-10 h-10\" />\r\n                <div>\r\n                  <div className=\"text-3xl font-bold\">9+</div>\r\n                  <div className=\"text-sm opacity-90\">Years Experience</div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          {/* Right - Content */}\r\n          <div className=\"space-y-6 fade-in-up animate-delay-200\">\r\n            <div className=\"inline-block bg-primary/10 text-primary px-4 py-2 rounded-full text-sm font-semibold\">\r\n              About Doctor\r\n            </div>\r\n            \r\n            <h2 className=\"section-title\">\r\n              About Dr. Samer Farhat\r\n            </h2>\r\n\r\n            <div className=\"space-y-4 text-muted-foreground text-lg\">\r\n              <p>\r\n                Dr. Samer Farhat is a highly experienced dentist specializing in advanced\r\n                dental care with over 9 years of expertise in transforming smiles and\r\n                improving oral health.\r\n              </p>\r\n              \r\n              <p>\r\n                With specialized training in <strong className=\"text-foreground\">Endodontics</strong> (Root Canal Therapy),\r\n                <strong className=\"text-foreground\"> Hollywood Smile & Veneers</strong>,\r\n                <strong className=\"text-foreground\"> Dental Implants</strong>, and\r\n                <strong className=\"text-foreground\"> Cosmetic Dentistry</strong>, Dr. Farhat combines\r\n                cutting-edge technology with personalized care to deliver exceptional results.\r\n              </p>\r\n\r\n              <p>\r\n                Located in the heart of Beirut, our clinic is equipped with state-of-the-art\r\n                facilities to ensure your comfort and the highest standard of dental treatment.\r\n              </p>\r\n            </div>\r\n\r\n            <Button\r\n              onClick={scrollToAppointment}\r\n              size=\"lg\"\r\n              className=\"btn-primary-medical\"\r\n            >\r\n              Book an Appointment\r\n            </Button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  );\r\n};\r\n\r\nexport default About;"
